{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "jsx": "react",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "moduleResolution": "node", // 모듈 해석 방법 설정
      "esModuleInterop": true     // ES 모듈 호환성 설정
    },
    "include": ["src/**/*"]        // 컴파일에 포함할 파일들
  }